<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/mypasttrip_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/img_home_book"
        android:layout_marginStart="15dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/mypasttrip_date_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_8"
        android:fontFamily="@font/notosans_kr_medium"
        android:includeFontPadding="false"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/mypasttrip_iv"
        app:layout_constraintEnd_toEndOf="@id/mypasttrip_iv"
        app:layout_constraintStart_toStartOf="@id/mypasttrip_iv"/>

    <TextView
        android:id="@+id/mypasttrip_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_8"
        android:fontFamily="@font/notosans_kr_medium"
        android:includeFontPadding="false"
        app:layout_constraintTop_toBottomOf="@id/mypasttrip_date_tv"
        app:layout_constraintEnd_toEndOf="@id/mypasttrip_iv"
        app:layout_constraintStart_toStartOf="@id/mypasttrip_iv"/>

</androidx.constraintlayout.widget.ConstraintLayout>